{
  "name": "bedita-sdk-js",
  "version": "4.0.0-beta.1",
  "description": "BEdita SDK for JavaScript.",
  "repository": {
    "type": "git",
    "url": "https://github.com/bedita/bedita-sdk-js.git"
  },
  "keywords": [
    "bedita",
    "models",
    "api"
  ],
  "main": "dist/bedita-sdk.js",
  "module": "src/index.js",
  "scripts": {
    "start": "rna bootstrap + build + build demo/demo/demo.js --output demo/index.js + build demo/demo/demo.scss --output demo/style.css + serve demo/ --watch + watch src/ + watch demo/demo",
    "build": "rna build --production",
    "watch": "rna watch src/",
    "clean": "rna bootstrap",
    "postinstall": "rm -f node_modules/**/*/.babelrc"
  },
  "private": true,
  "license": "LGPL-3.0",
  "author": "BEdita <info@bedita.com> (http://www.bedita.com)",
  "dependencies": {
    "@chialab/schema-model": "github:chialab/schema-model#1.3.2",
    "object-path": "^0.11.4"
  }
}
